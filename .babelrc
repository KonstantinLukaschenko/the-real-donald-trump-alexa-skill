{
  "compact": false,
  "presets": [["env", { "targets": { "node": 4.6 } }]],
  "plugins": [
    "transform-decorators-legacy",
    [
      "transform-react-jsx",
      {
        "pragma": "ssml"
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./src",
          "./config"

        ],
        "extensions": [
          "",
          ".js",
          ".jsx"
        ]
      }
    ]
  ]
}
